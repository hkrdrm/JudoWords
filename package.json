{
  "name": "SnapConnect",
  "private": true,
  "version": "0.0.1",
  "description": "Security Proposal Sales app.",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist/",
    "copy_beta": "node copy_index.js",
    "copy_dist": "cp src/prod.index.html dist/index.html & cp src/data/orange_belt.pdf dist/orange_belt.pdf & cp src/data/green_belt.pdf dist/green_belt.pdf",
    "dist": "npm run copy_dist & webpack --env=dist",
    "beta": "npm run copy_beta & webpack --env=beta",
    "copy:local": "node copy_to_localhost.js",
    "copy:beta": "node copy_to_tpiweb1-beta.js",
    "copy:nginx": "cp -rf dist/* /var/www/html/SnapConnect-beta/",
    "copy:ubuntuToBeta": "cp -rf dist/* /media/tpiweb1/SnapConnect-beta/",
    "copy:ubuntuToProd": "cp -rf dist/* /media/tpiweb1/SnapConnect-prod/",
    "copy:prod": "node copy_to_tpiweb1-prod.js",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "node server.js --env=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
